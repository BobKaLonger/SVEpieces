		{
			"Action": "Load",
			"Target": "Buildings/SpaciousCoop",
			"FromFile": "Buildings/SpaciousCoop.png"
		},
		{
			"Action": "Load",
			"Target": "Buildings/SpaciousBarn",
			"FromFile": "Buildings/SpaciousBarn.png"
		},
		
		// add location data
		{
			"Action": "EditData",
			"Target": "Data/Locations",
			"Entries": {
				"SpaciousBarn": {
					"CreateOnLoad": {
						"MapPath": "Maps/SpaciousBarn"
					}
				},
				"SpaciousCoop": {
					"CreateOnLoad": {
						"MapPath": "Maps/SpaciousCoop"
					}
				}
			}
		},

		// add the map files
		{
            "Action": "Load",
            "Target": "Maps/SpaciousBarn",
            "FromFile": "Maps/Buildings/SpaciousBarn.tmx"
        },
		{
            "Action": "Load",
            "Target": "Maps/SpaciousCoop",
            "FromFile": "Maps/Buildings/SpaciousCoop.tmx"
        },

		//load paint masks from SVE
